{
  "name": "fastgpt",
  "version": "4.0",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "format-code": "prettier --config \"./.prettierrc.js\" --write \"./**/src/**/*.{ts,tsx,scss}\"",
    "format-doc": "zhlint --dir ./docSite *.md --fix"
  },
  "devDependencies": {
    "@types/multer": "^1.4.10",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "prettier": "^3.0.3",
    "zhlint": "^0.7.1"
  },
  "lint-staged": {
    "./**/**/*.{ts,tsx,scss}": "npm run format-code",
    "./**/**/*.md": "npm run format-doc"
  },
  "dependencies": {
    "multer": "1.4.5-lts.1",
    "openai": "4.23.0",
    "axios": "^1.5.1",
    "nanoid": "^4.0.1",
    "next": "13.5.2",
    "dayjs": "^1.11.7",
    "i18next": "^22.5.1",
    "next-i18next": "^13.3.0",
    "react-i18next": "^12.3.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.6.0"
  }
}
